{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|test/|docs)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "referenceTitle": "reduce.ac Docs",
    "disableSort": true,
    "collapse": true,
    "resources": {
      "@thi.ng/umbrella": "http://thi.ng/umbrella"
    }
  },
  "opts": {
    "destination": "docs",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc/templates/jsdoc-template"
  }
}